class person:

   def __init__(self, first_name, last_name,age):
    #intance variables
    #print('init method called')

    self.first_name = first_name
    self.last_name = last_name
    self.age = age

p1 = person('Aman','Raj',23)
p2 = person('vicky','Kumar',23)

print(p1.first_name)
print(p2.first_name)
